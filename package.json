{
  "name": "css-tab-completions",
  "version": "1.0.0",
  "description": "",
  "main": "dist/extensionMain.js",
  "scripts": {
    "dev": "tsc -w",
    "test": "jest"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:css-tab-completion"
  ],
  "contributes": {
    "commands": [
      {
        "command": "css-tab-completion",
        "title": "CSS Tab Completion"
      }
    ],
    "keybindings": [
      {
        "command": "css-tab-completion",
        "key": "Tab",
        "when": "!suggestWidgetVisible"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/vscode": "^1.50.0",
    "fs-extra": "^9.0.1",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  }
}
