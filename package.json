{
  "name": "css-tab-completions",
  "version": "1.0.0",
  "description": "",
  "main": "dist/extensionMain.js",
  "scripts": {
    "dev": "tsc -w"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "css-tab-completion",
        "title": "CSS Tab Completion"
      }
    ],
    "keybindings": [
      {
        "command": "css-tab-completion",
        "key": "Tab",
        "when": "!suggestWidgetVisible"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/vscode": "^1.50.0",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  }
}
